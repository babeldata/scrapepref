# Configuration AWS S3
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_REGION=us-east-1
S3_BUCKET_NAME=your_bucket_name_here

# Pour MinIO ou autre S3-compatible: sp√©cifier l'URL
# Exemples: http://localhost:9000 ou https://minio.example.com
# Laisser vide pour AWS S3 standard
S3_ENDPOINT_URL=

# Configuration du scraper
SCRAPE_DELAY_SECONDS=2
MAX_CONCURRENT_PAGES=5
MAX_PAGES_TO_SCRAPE=0  # 0 = toutes les pages
PAGE_LOAD_TIMEOUT=90000  # 90 secondes (en millisecondes)
PDF_DOWNLOAD_TIMEOUT=60000  # 60 secondes (en millisecondes)

# Mode test (sans upload S3)
DRY_RUN=false

